                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select
	jcjm.JCCo as Company
,	jcjm.Job as JobNumber
,	jcjm.Description as JobDescription
,	ddci.DisplayValue as  JobStatus
,	jcmp.Name as ProjectManager
,	jcjm.udProjStart as ProjectStartDate
,	jcjm.udProjEnd as ProjectEndDate
,	CAST(jcjm.JCCo AS VARCHAR(3)) + '.' + CAST(jcjm.Job AS varchar(10)) AS JobKey
from
	JCJM jcjm left outer join
	JCMP jcmp on
		jcjm.JCCo=jcmp.JCCo
	and jcjm.ProjectMgr=jcmp.ProjectMgr left outer join
	DDCIShared ddci on
		jcjm.JobStatus=ddci.DatabaseValue
	and ddci.ComboType = 'JCJMJobStatus'	
where
	jcjm.JCCo in (1,20)
and jcjm.JobStatus in (0,1,2)
and ( jcjm.udDateChanged &gt;= ? )
order by
	jcjm.JCCo, jcjm.Job</property>
