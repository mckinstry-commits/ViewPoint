  <DataSets>
    <DataSet Name="SMagreementSite">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SMCo">
            <Value>=Parameters!SMCo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Agreement">
            <Value>=Parameters!Agreement.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Revision">
            <Value>=Parameters!Revision.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
Author: Peter Knudson
Date: 12/20/2019
Description:  This is used in a subreport of the Agreement Renewal report.  Its main purpose is to get site notes which are then displayed in the sub report
*/

--declare @SMCo int = 1
--declare @Agreement varchar(100) = '10241'
--declare @Revision varchar(100) = '2'

select sss.Notes, sss.Description, sss.ServiceSite
FROM SMServiceSite sss 
WHERE sss.SMCo = @SMCo AND sss.ServiceSite IN 
(SELECT DISTINCT sas.ServiceSite
FROM SMAgreementService sas
where sas.Agreement = @Agreement
and sas.Revision = @Revision
and sas.SMCo = @SMCo)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
