    <DataSet Name="WorkOrderScopes">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SMCo">
            <Value>=Parameters!SMCo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Agreement">
            <Value>=Parameters!Agreement.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ExpDate">
            <Value>=Parameters!ExpDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Adapted from subWorkOrderScope by curts
2019-11-25 ejs the parent call to this differs in that this is for agreements that have expired or are about to expire
and fewer parameters are provided.  Formatting of case statements, added description block, and SSMS parameter block, and Agreement join 

*/

/* SSMS parameter block
DECLARE @SMCo TINYINT = 1
--DECLARE @Agreement VARCHAR(100) = '10133'
DECLARE @Agreement VARCHAR(100) = ''
----DECLARE @Revision INT = 1
----DECLARE @StartDate DATETIME = '2018-01-01'
----DECLARE @EndDate DATETIME = '2019-04-01'
declare @ExpDate datetime = '2019-10-01'
*/


SELECT d.SMCo, d.WorkOrder, d.Scope, d.Agreement, d.Revision, d.WorkScope, d.[Description], d.[Status], d.PriceMethod, 
	d.DueStartDate, d.DueEndDate, d.Division, d.CustomerPO, d.BillToARCustomer, d.CallType, d.PriorityName, d.ServiceItem, d.Price,
	d.TaxType, d.TaxCode, d.ReadyToBill, d.ServiceCenter, d.Notes, d.RateTemplate, d.NotToExceed, d.Phase, d.JCCo,
	d.Job, d.[Service], d.IsPreventativeMaintenance, d.WODescription, d.WOStatus, d.RequestedDate, d.EnteredDateTime, 
	d.WODate, d.FormattedWODate, d.ScopeHours, d.def , d.DateTerminated, d.ExpirationDate, d.NonExpiring
FROM
(
SELECT 1 def, s.SMCo, s.WorkOrder, s.Scope, 
	s.Agreement, s.Revision, 
	s.WorkScope, s.[Description], 

	CASE s.Status WHEN 1 THEN 'Open' 
	              WHEN 2 THEN 'On Hold' 
				  WHEN 3 THEN 'Complete' 
				  WHEN 4 THEN 'Closed' 
				  ELSE 'N/A' END [Status] , 

	CASE s.PriceMethod WHEN 'N' THEN 'Non-Billable' 
	                   WHEN 'T' THEN 'Time &amp; Materials' 
					   WHEN 'F' THEN 'Flat Price' 
					   ELSE 'N/A' END [PriceMethod], 

	s.DueStartDate, s.DueEndDate, s.Division, s.CustomerPO, s.BillToARCustomer, 
	s.CallType, s.PriorityName, s.ServiceItem, s.Price, s.TaxType, s.TaxCode, s.ReadyToBill, s.ServiceCenter, s.Notes, s.RateTemplate, s.NotToExceed, s.Phase, s.JCCo, 
	s.Job, s.Service, s.IsPreventativeMaintenance,
	wo.[Description] [WODescription], st.Status [WOStatus], wo.RequestedDate, wo.EnteredDateTime, 
	COALESCE(wo.RequestedDate, wo.EnteredDateTime) [WODate],
	CONVERT(VARCHAR(12), COALESCE(wo.RequestedDate, wo.EnteredDateTime), 107) [FormattedWODate],
	COALESCE(q.TotalHours, 0) [ScopeHours]
	, a.DateTerminated, a.ExpirationDate, a.NonExpiring
from dbo.HQCO hq (nolock)
join SMAgreementExtended a on a.SMCo=hq.HQCo and a.CurrentRevision=a.Revision --added 2019-11-25 ejs
join SMWorkOrderScope s (NOLOCK) ON s.SMCo = a.SMCo and s.Agreement=a.Agreement --changed 2019-11-25 ejs
JOIN SMWorkOrder wo (NOLOCK) ON wo.SMCo = s.SMCo and wo.WorkOrder = s.WorkOrder
JOIN SMWorkOrderStatus st (NOLOCK) ON st.SMCo = wo.SMCo AND st.WorkOrder = wo.WorkOrder
LEFT JOIN 
	(SELECT SUM(wc.CostQuantity) [TotalHours], SMCo, WorkOrder FROM SMWorkCompleted wc 
	WHERE wc.Type = 2 AND wc.SMCostType = 1 GROUP BY SMCo, WorkOrder) q ON q.SMCo = wo.SMCo AND q.WorkOrder = wo.WorkOrder

WHERE s.SMCo = @SMCo --AND s.Agreement = @Agreement and s.Revision = @Revision -- already limited by CurrentRevision, switching to multi agreement possibility ejs 2019-11-25
and
	  (a.Agreement in (select splitdata from dbo.fnSplitString(@Agreement,',')) or len(@Agreement)=0)
AND COALESCE(s.WorkScope, '') &lt;&gt; '006' -- no truck charge scopes

UNION ALL
SELECT 0 def, NULL SMCo, NULL WorkOrder, NULL Scope, NULL Agreement, NULL Revision, NULL WorkScope, NULL [Description], NULL [Status], NULL PriceMethod, 
	NULL DueStartDate, NULL DueEndDate, NULL Division, NULL CustomerPO, NULL BillToARCustomer, NULL CallType, NULL PriorityName, NULL ServiceItem, NULL Price,
	NULL TaxType, NULL TaxCode, NULL ReadyToBill, NULL ServiceCenter, NULL Notes, NULL RateTemplate, NULL NotToExceed, NULL Phase, NULL JCCo,
	NULL Job, NULL [Service], NULL IsPreventativeMaintenance, NULL WODescription, NULL WOStatus, NULL RequestedDate, NULL EnteredDateTime, 
	NULL WODate, NULL FormattedWODate, 0 ScopeHours, null DateTerminated, null ExpirationDate, null NonExpiring
--Deprecated old date parameters --ejs 2019-11-25
--) d WHERE d.def = 0 OR( @StartDate = '' OR (@StartDate &lt;&gt; '' AND d.WODate &gt;= CAST(@StartDate AS DATETIME) AND d.WODate &lt; CAST(@EndDate AS DATETIME)))
) d WHERE d.def = 0 or isnull(@ExpDate,'') = '' OR d.ExpirationDate &gt;= @ExpDate 


order by def desc, Agreement, DueEndDate</CommandText>
      </Query>
