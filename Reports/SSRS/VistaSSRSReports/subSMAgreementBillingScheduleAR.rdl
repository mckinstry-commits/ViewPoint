  <DataSets>
    <DataSet Name="SMagreementBillingSchedule">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SMCo">
            <Value>=Parameters!SMCo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Agreement">
            <Value>=Parameters!Agreement.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
Created by Elizabeth Jayne Stitt 2019/11/26 
*/

--declare @SMCo int = 1
--declare @Agreement varchar(100) = '10241'


SELECT a.[SMAgreementBillingScheduleID]
      ,a.[SMCo]
      ,a.[Agreement]
      ,a.[Revision]
      ,a.[Service]
      ,a.[Billing]
      ,a.[Date]
      ,a.[Month]
      ,a.[Day]
      ,a.[BillingAmount]
      ,a.[SMInvoiceID]
      ,a.[Notes]
      ,a.[UniqueAttchID]
      ,a.[TaxType]
      ,a.[TaxGroup]
      ,a.[TaxCode]
      ,a.[TaxBasis]
      ,a.[TaxAmount]
      ,a.[BillingType]
      ,a.[AdjustmentRevision]
      ,a.[Department]
      ,a.[BillingSequence]
      ,a.[BillingCount]
	  
, i.InvoiceNumber, i.BillToARCustomer, i.Invoiced, i.BatchMonth, i.BillEmail, i.InvoiceDate 
, ae.udBillingNotes
  
  FROM HQCO h --Used to restrict to vista company access
join [dbo].[SMAgreementBillingScheduleExt] a on h.HQCo=a.SMCo
join [dbo].[SMAgreementExtended] ae on ae.SMCo=a.SMCo and ae.Agreement = a.Agreement and ae.Revision=a.Revision
left join dbo.SMInvoice i on i.SMCo=a.SMCo and a.SMInvoiceID=i.SMInvoiceID

where 1=1 and h.HQCo=@SMCo

and 
	  (a.Agreement in (select splitdata from dbo.fnSplitString(@Agreement,',')) or Len(@Agreement)=0 ) --SSRS</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
