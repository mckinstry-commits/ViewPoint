        </QueryParameters>
        <CommandText>/*
Adapted from SSRS subWorkCompleted.rdl that was created by curts
2019-11-25 ejs the parent call to this differs in that this is for agreements that have expired or are about to expire
and fewer parameters are provided.  Formatting, added description block, and SSMS parameter block are the only present changes
2019-11-25 ejs remove "Type" limit
*/

/* SSMS paramater block uncomment for SSRS by -- in front of * pairs
declare @SMCo int = 1
declare @Scope int = 1
declare @WorkOrder int =9531975--9539956
*/

SELECT ROW_NUMBER ( ) OVER (PARTITION BY d.Type ORDER BY d.Type, d.WorkCompleted) [LineNumber],
 d.WorkCompleted, d.WorkOrder, d.SMCo, d.[Type], d.Technician, d.Date, d.Hours, d.PO, d.PODescription, d.StandardItem, d.TechName, d.Description, d.FormattedWCDate, d.SMCostType, d.ActualCost
FROM
(
SELECT
c.WorkCompleted, c.WorkOrder, c.SMCo,
CASE WHEN c.Type = 1 THEN 'Equip' 
	 WHEN c.Type = 2 AND c.SMCostType = 1 THEN 'Labor' 
	 WHEN c.Type = 2 AND c.SMCostType = 6 THEN 'Expense' 
	 WHEN c.Type = 3 THEN 'Misc' 
	 WHEN c.Type = 4 THEN 'Inventory' 
	 WHEN c.Type = 5 THEN 'Purchase' ELSE 'N/A' END [Type],

c.Technician, c.Date, lab.CostQuantity [Hours], 
pur.PO, pur.Description [PODescription], 
msc.StandardItem, 
'Employee ' + COALESCE(CAST(t.Employee AS VARCHAR(12)), 'n/a') TechName, c.Description,
CONVERT(VARCHAR(12), c.Date, 107) [FormattedWCDate],
c.SMCostType,
c.ActualCost --Added
FROM HQCO hq (NOLOCK)
JOIN SMWorkOrder wo (NOLOCK) ON wo.SMCo = hq.HQCo
JOIN SMWorkCompleted c (NOLOCK) ON c.SMCo = wo.SMCo AND c.WorkOrder= wo.WorkOrder
LEFT JOIN dbo.SMWorkCompletedLabor lab (NOLOCK) ON lab.SMCo = c.SMCo    AND lab.WorkOrder = c.WorkOrder AND lab.WorkCompleted = c.WorkCompleted
LEFT JOIN dbo.SMWorkCompletedMisc msc (NOLOCK) ON msc.SMCo = c.SMCo     AND msc.WorkOrder = c.WorkOrder AND msc.WorkCompleted = c.WorkCompleted
LEFT JOIN dbo.SMWorkCompletedPurchase pur (NOLOCK) ON pur.SMCo = c.SMCo AND pur.WorkOrder = c.WorkOrder AND pur.WorkCompleted = c.WorkCompleted
LEFT JOIN SMTechnician t (NOLOCK) ON t.SMCo = c.SMCo AND t.Technician = c.Technician
WHERE wo.SMCo = @SMCo AND wo.WorkOrder = @WorkOrder AND c.Scope = @Scope
) d
--WHERE d.Type IN ('Labor', 'Purchase', 'Misc') --ejs 2019-11-27 removed
ORDER BY d.Type, d.WorkCompleted</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
