Declare @PREHNames Table (Employee bEmployee, FullName varchar(83), ActiveYN bYN, PRCo bCompany, Email varchar(60), RowN int, PRIMARY KEY (PRCo asc, Employee ASC))
--select * from @SMASdiv
-- Get one distinct record for Employee 
insert @PREHNames
Select * from (

select * from (
select *, ROW_NUMBER() over(Partition by udAccountExec order by udAccountExec desc, ActiveYN desc) RowN from (  
select  udAccountExec, p.FullName, ActiveYN, PRCo, Email from PREHFullName p
Join (select distinct udAccountExec from dbo.SMServiceSite) s on p.Employee = s.udAccountExec) q
) AE1
where AE1.RowN=1
union
select * from (
select *, ROW_NUMBER() over(Partition by udAccountExec2 order by udAccountExec2 desc, ActiveYN desc) RowN from (  
select  udAccountExec2, p.FullName, ActiveYN, PRCo, Email from PREHFullName p
Join (select distinct udAccountExec2 from dbo.SMServiceSite) s on p.Employee = s.udAccountExec2) q
) AE2
where AE2.RowN=1
union

select * from (
select *, ROW_NUMBER() over(Partition by udAccountMgr order by udAccountMgr desc, ActiveYN desc) RowN from (  
select  udAccountMgr, p.FullName, ActiveYN, PRCo, Email from PREHFullName p
Join (select distinct udAccountMgr from dbo.SMServiceSite) s on p.Employee = s.udAccountMgr) q
) AM1
where AM1.RowN=1
union
select * from (
select *, ROW_NUMBER() over(Partition by udAccountMgr2 order by udAccountMgr2 desc, ActiveYN desc) RowN from (  
select  udAccountMgr2, p.FullName, ActiveYN, PRCo, Email from PREHFullName p
Join (select distinct udAccountMgr2 from dbo.SMServiceSite) s on p.Employee = s.udAccountMgr2) q
) AM2
where AM2.RowN=1
) q

order by ActiveYN, FullName
--where q.ActiveYN='Y'

--select * from @PREHNames

Declare @Invoices Table(
	
[AmountDue] [dbo].[bDollar] NOT NULL,
[CreditAmt] [dbo].[bDollar] NOT NULL,
[Paid] [dbo].[bDollar] NOT NULL,
[Retainage] [dbo].[bDollar] NOT NULL,
[ARTransType] [char](1) NOT NULL,
[Invoice] [varchar](10) NULL,
[Notes] [varchar](max) NULL,
[ARPostedMth] [dbo].[bMonth] NOT NULL,
[SMCo] [dbo].[bCompany] NOT NULL,
[ARTrans] [dbo].[bTrans] NOT NULL,
[ARCo] [dbo].[bCompany] NOT NULL,
[Customer] [dbo].[bCustomer] NOT NULL,
[DueDate] [dbo].[bDate] NULL,
[InvoiceDate] [dbo].[bDate] NOT NULL,
[InvoiceType] [char](1) NOT NULL,
[WorkOrders] [nvarchar](max) NULL,
[TotalInvoiced] [numeric](38, 2) NULL,
[WorkOrderIL] [int] NULL
 primary key (ARCo asc, ARTrans asc, ARPostedMth asc) )

Insert into @Invoices 

 Select
 h.AmountDue
,h.CreditAmt
,h.Paid
,h.Retainage
,h.ARTransType
,h.Invoice
,h.Notes
,i.ARPostedMth,i.SMCo,i.ARTrans,i.ARCo
,i.Customer
,i.DueDate
,i.InvoiceDate
,i.InvoiceType
,l.InvoiceWorkOrders WorkOrders
,l.TotalAmount TotalInvoiced 
,isnull(l.WorkOrder,left(InvoiceWorkOrders,7)) WorkOrderIL
from dbo.SMInvoiceList l  (nolock)
join dbo.SMInvoice i (nolock) on l.ARCo=i.ARCo and l.Invoice=i.Invoice
join dbo.ARTH h  (nolock) on l.ARCo=h.ARCo and h.ARTrans=l.ARTrans and h.Mth=l.ARPostedMth

where 1=1
and i.ARPostedMth between @BOY and @AsofDate
	--and (
	--   (i.InvoiceDate between @mStartDate and @mEndDate and @StartDate is null and @EndDate is null )
	--or (@DateRange='NONE' and @StartDate is null and @EndDate is null)
	--or (@StartDate is null and i.InvoiceDate  &lt;= @EndDate)
	--or (@EndDate is null and i.InvoiceDate  &gt;= @StartDate)
	--or (i.InvoiceDate   between @StartDate and @EndDate )
	--    )


--select * from @Invoices

Declare @WorkOrderInfo Table (SMCo bCompany not null, WorkOrder int not null, ServiceSite varchar(20) not null, ServiceCenter varchar(10) not null,TrueCustomer bCustomer,TrueCustGroup bGroup, ServiceSiteDescriptionWO varchar(60) 
, udAccountMgr int, udAccountMgr2 int, udAccountExec int, udAccountExec2 int, udGLDept varchar(200), DeptDescription varchar(400)
primary key (SMCo asc, WorkOrder asc) )
insert into @WorkOrderInfo 
select wo.SMCo, wo.WorkOrder, wo.ServiceSite, wo.ServiceCenter,c.TrueCustomer,c.TrueCustGroup, cast(c.TrueDescription as varchar(60)) ServiceSiteDescriptionWO , udAccountMgr int, udAccountMgr2 int, udAccountExec int, udAccountExec2 int ,udGLDept, DeptDescription
from dbo.SMWorkOrder wo  (nolock) 
  join dbo.[vrvSMServiceSiteCustomer] c   (nolock) on c.SMCo=wo.SMCo and c.ServiceSite=wo.ServiceSite 
  left join @uGLDept gl  on gl.SMCo=wo.SMCo and wo.WorkOrder=gl.WorkOrder
  order by 1,2
--select * from @WorkOrderInfo

Declare @AgreementInfo Table (
SMCo bCompany not null
, Invoice int not null
, Agreement varchar(5) not null
, Revision int not null
, [Version] varchar(7) not null
, ServiceSite varchar(20) not null
, ServiceCenter varchar(10) not null 
, TrueCustomer bCustomer
, TrueCustGroup bGroup
, ServiceSiteDescriptionSA  varchar(60) 
, CM2Name varchar(60)
, udAccountMgr int, udAccountMgr2 int, udAccountExec int, udAccountExec2 int
, udGLDept varchar(200), DeptDescription varchar(400)
primary key (SMCo asc, Invoice asc, Agreement asc, Revision asc, ServiceSite asc, ServiceCenter asc) 
) 

insert into @AgreementInfo
select d.SMCo, i.InvoiceNumber Invoice, d.Agreement , a.Revision, a.[Version],c2.ServiceSite, sa.ServiceCenter, c2.TrueCustomer, c2.TrueCustGroup, c2.TrueDescription saServiceSiteDescription, cm2.Name CM2Name  
,udAccountMgr, udAccountMgr2, udAccountExec, udAccountExec2, gl2.udGLDept, gl2.DeptDescription
from dbo.SMInvoice i
join dbo.SMInvoiceDetail d  (nolock) on d.SMCo=i.SMCo and d.Invoice=i.Invoice and i.InvoiceType='A'
join dbo.SMAgreement a  (nolock) on a.SMCo=d.SMCo and a.Agreement=d.Agreement and a.Version=d.Version
join (select distinct SMCo, Agreement, Revision, ServiceSite, ServiceCenter from dbo.SMAgreementService (nolock)) sa  on sa.Agreement=d.Agreement and sa.Revision=a.Revision and sa.SMCo=d.SMCo 
join dbo.[vrvSMServiceSiteCustomer] c2  (nolock) on c2.SMCo = sa.SMCo and c2.ServiceSite=sa.ServiceSite
left join dbo.ARCM cm2  (nolock) on cm2.CustGroup=c2.TrueCustGroup and cm2.Customer=c2.TrueCustomer 
left join @uGLAgDept gl2 on gl2.SMCo=d.SMCo and gl2.Agreement=d.Agreement and gl2.[Version] = d.[Version]


select i.*, pm1.FullName AM1Name, pm2.FullName AM2Name, pe1.FullName AE1Name, pe2.FullName AE2Name from (
 select 
 i.* 
,wo.WorkOrder
,a.Agreement
,a.[Version]
,isnull(cm.Name,a.CM2Name) CustomerName
,isnull(wo.ServiceCenter,a.ServiceCenter) ServiceCenter
,isnull(wo.ServiceSite,a.ServiceSite) ServiceSite
,isnull(wo.ServiceSiteDescriptionWO,a.ServiceSiteDescriptionSA) ServiceSiteDescription
,isnull(wo.udAccountMgr, a.udAccountMgr) udAccountMgr, isnull(wo.udAccountMgr2, a.udAccountMgr2) udAccountMgr2, isnull(wo.udAccountExec, a.udAccountExec) udAccountExec, isnull(wo.udAccountExec2, a.udAccountExec2) udAccountExec2

,coalesce(wo.udGLDept,a.udGLDept) Dept#, coalesce(  wo.DeptDescription,a.DeptDescription) DeptDescription

from @Invoices i
left join @WorkOrderInfo wo on wo.SMCo=i.SMCo and wo.WorkOrder=i.WorkOrderIL
left join dbo.ARCM cm  (nolock) on cm.CustGroup=wo.TrueCustGroup and cm.Customer=wo.TrueCustomer
left join @AgreementInfo a on a.SMCo=i.SMCo and a.Invoice=i.Invoice
) i
left join @PREHNames pm1 on pm1.Employee=i.udAccountMgr
left join @PREHNames pm2 on pm2.Employee=i.udAccountMgr2
left join @PREHNames pe1 on pe1.Employee=i.udAccountExec
left join @PREHNames pe2 on pe2.Employee=i.udAccountExec2

where 1=1 and i.ARCo=@SMCo
and i.ARPostedMth between @BOY and @AsofDate
--and ( (isnull('AM1:'+pm1.FullName + ' ','') + isnull('AE1:'+pe1.FullName + ' ','') + isnull('AM2:'+pm2.FullName + ' ','') + isnull('AE2:'+pe2.FullName + ' ','') ) &lt;&gt; '')
 ----SSMS BLock
	--and (--SSMS
	--(  (i.udAccountMgr in (select splitdata from dbo.fnSplitString(@AMgr,',')) )  --SSMS
	--or (i.udAccountMgr2 in (select splitdata from dbo.fnSplitString(@AMgr,',')) ) --SSMS
	--or (i.udAccountExec in (select splitdata from dbo.fnSplitString(@AMgr,',')) ) --SSMS
	--or (i.udAccountExec2 in (select splitdata from dbo.fnSplitString(@AMgr,',')) ) --SSMS
	--or (-1 in (select splitdata from dbo.fnSplitString(@AMgr,',')) and coalesce(i.udAccountMgr,i.udAccountMgr2,i.udAccountExec,i.udAccountExec2,0) &lt;&gt; 0)--SSMS
	--) 
	--or (coalesce(i.udAccountMgr,i.udAccountMgr2,i.udAccountExec,i.udAccountExec2,0) = 0 and -2 in (select splitdata from dbo.fnSplitString(@AMgr,',')))--SSMS
	--)
----End SSMS Block

----SSRS Block	
	and (
	   (i.udAccountMgr in (@AMgr) or i.udAccountMgr2 in (@AMgr) or i.udAccountExec in (@AMgr) or i.udAccountExec2 in (@AMgr) or (-1 IN (@AMgr) and coalesce(i.udAccountMgr,i.udAccountMgr2,i.udAccountExec,i.udAccountExec2,0)&lt;&gt;0)
	   or (coalesce(i.udAccountMgr,i.udAccountMgr2,i.udAccountExec,i.udAccountExec2,0) = 0 and -2 in (@AMgr))
	    )) --SSRS
----End SSRS Block
--where InvoiceType='A'

