Declare @SMDVdup table(SMCo	bCompany, WorkOrder int,  Scnt int null, Dcnt int null,  PRIMARY KEY ([SMCo] ASC, WorkOrder ASC))

--First find multicount service center/ division record counts
Insert @SMDVdup
Select  
      wo.SMCo, wo.WorkOrder
	, count(distinct wos.ServiceCenter) SCnt
	, count(distinct Division) DCnt

	from dbo.SMWorkOrder wo (nolock) 
	join dbo.SMWorkOrderScope wos (nolock) on wos.WorkOrder=wo.WorkOrder and wos.SMCo=wo.SMCo
	group by wo.SMCo, wo.WorkOrder
	having count(distinct wos.ServiceCenter) &gt; 1 or count(distinct Division) &gt;1

--	select * from @SMDVdup --debug test

--Next get all work orders with only one distinct combination of service center or division in the work order scope details
--106704 --2018-11-09

Declare @uGLDept Table(udGLDept varchar(200), DeptDescription varchar(400), SMCo int, WorkOrder int, ServiceCenter varchar(200), Division Varchar(200) primary key (SMCo ASC, WorkOrder ASC, ServiceCenter ASC, Division))
insert into
@uGLDept

select dp.udGLDept, dp.Description, sc.* from dbo.SMDivision dv (nolock) 
join (select distinct SMCo, WorkOrder,ServiceCenter,Division from dbo.SMWorkOrderScope (nolock) ) sc  on sc.SMCo=dv.SMCo and sc.ServiceCenter=dv.ServiceCenter and sc.Division=dv.Division 
join dbo.SMDepartment dp (nolock) on dp.SMCo=sc.SMCo and dp.Department=dv.Department
where WorkOrder not in (select WorkOrder from @SMDVdup)

--select * from @uGLDept --debug test

--Next Get all work orders that have more than one distinct service center or division
Declare @uGLDept2 Table(udGLDept varchar(200), DeptDescription varchar(400), SMCo int, WorkOrder int, ServiceCenter varchar(200), Division Varchar(200) primary key (SMCo ASC, WorkOrder ASC, ServiceCenter ASC, Division))
insert into @uGLDept2
select dp.udGLDept, dp.Description, sc.* from dbo.SMDivision dv (nolock) 
join (select distinct SMCo, WorkOrder,ServiceCenter,Division from dbo.SMWorkOrderScope (nolock)  ) sc on sc.SMCo=dv.SMCo and sc.ServiceCenter=dv.ServiceCenter and sc.Division=dv.Division 
join dbo.SMDepartment dp (nolock) on dp.SMCo=sc.SMCo and dp.Department=dv.Department
where WorkOrder in (select WorkOrder from @SMDVdup)

-- select * from @uGLDept2 --debug test

 --Merge them together

 insert into
@uGLDept

select distinct GLDept, DeptDesciption, SMCo, WorkOrder,ServiceCenter,Division  from (
SELECT	DISTINCT SMCo, WorkOrder, 
		GLDept = STUFF(( SELECT	distinct ',' + [udGLDept]
				FROM	@uGLDept2 AS B2
				WHERE B2.WorkOrder = B1.WorkOrder	
				--ORDER BY WorkOrder
				FOR XML	PATH('')
				), 1, 1, '')
				,		
		DeptDesciption = STUFF(( SELECT	distinct ',' + [DeptDescription]
				FROM	@uGLDept2 AS B2
				WHERE B2.WorkOrder = B1.WorkOrder	
				--ORDER BY WorkOrder
				FOR XML	PATH('')
				), 1, 1, '')
				,
		Division = STUFF(( SELECT distinct ',' + [Division]
				FROM	@uGLDept2 AS B2
				WHERE B2.WorkOrder = B1.WorkOrder	
				--ORDER BY WorkOrder
				FOR XML	PATH('')
				), 1, 1, '')
				,
		ServiceCenter = STUFF(( SELECT distinct ',' + ServiceCenter
				FROM	@uGLDept2 AS B2
				WHERE B2.WorkOrder = B1.WorkOrder	
				--ORDER BY WorkOrder
				FOR XML	PATH('')
				), 1, 1, '')

 from @uGLDept2 as B1 )  q

  --select * from @uGLDept where WorkOrder in (select WorkOrder from @SMDVdup) --debug test

  --we now have a GLDepts we can join to for work orders, now we need aggreements

Declare @SMASdup table(SMCo	bCompany, Agreement varchar(15), Revision int, [Version] varchar(7) ,Scnt int null, Dcnt int null, SScnt int null  PRIMARY KEY ([SMCo] ASC, [Agreement] ASC, [Revision] ASC))

--Find duplicate service center/ division record counts for agreements
Insert @SMASdup
Select  
      SMCo, Agreement, Revision, [Version]
	, count(distinct ServiceCenter) SCnt
	, count(distinct Division) DCnt
	, count(distinct ServiceSite) SScnt
	from SMAgreementService (nolock) group by SMCo, Agreement, Revision, [Version]--, ServiceCenter, Division 
	having count(distinct ServiceCenter) &gt; 1 or count(distinct Division) &gt;1 or count(distinct ServiceSite) &gt; 1
	--10119
--select * from @SMASdup --Debug test

--Next get all agreements with only one distinct combination

Declare @uGLAgDept Table(udGLDept varchar(200), DeptDescription varchar(400), SMCo int, Agreement varchar(15), [Version] varchar(7), ServiceSite varchar(200), ServiceCenter varchar(200), Division Varchar(200) primary key (SMCo ASC, Agreement ASC, [Version] ASC, ServiceSite ASC, ServiceCenter ASC, Division))
insert into
@uGLAgDept

select dp.udGLDept, dp.Description, sc.* from dbo.SMDivision dv (nolock) 
join (select distinct SMCo, Agreement, [Version], ServiceSite, ServiceCenter, Division from dbo.SMAgreementService (nolock) ) sc  on sc.SMCo=dv.SMCo and sc.ServiceCenter=dv.ServiceCenter and sc.Division=dv.Division 
join dbo.SMDepartment dp (nolock) on dp.SMCo=sc.SMCo and dp.Department=dv.Department
where Agreement not in (select Agreement from @SMASdup)

--select * from @uGLAgDept --Debug test


--Next get all Agreements that have more than one distinct service center, site, or Division
Declare @uGLAgDept2 Table(udGLDept varchar(200), DeptDescription varchar(400), SMCo int, Agreement varchar(15), [Version] varchar(7), ServiceSite varchar(200), ServiceCenter varchar(200), Division Varchar(200) primary key (SMCo ASC, Agreement ASC, [Version] ASC, ServiceSite ASC, ServiceCenter ASC, Division))
insert into
@uGLAgDept2

select dp.udGLDept, dp.Description, sc.* from dbo.SMDivision dv (nolock) 
join (select distinct SMCo, Agreement, [Version],  ServiceSite, ServiceCenter, Division from dbo.SMAgreementService (nolock) ) sc  on sc.SMCo=dv.SMCo and sc.ServiceCenter=dv.ServiceCenter and sc.Division=dv.Division 
join dbo.SMDepartment dp (nolock) on dp.SMCo=sc.SMCo and dp.Department=dv.Department
where Agreement  in (select Agreement from @SMASdup)

--select * from @uGLAgDept2 --Debug test

--Merge them together

 insert into
@uGLAgDept

select distinct GLDept, DeptDesciption, SMCo, Agreement, [Version], ServiceSite, ServiceCenter,Division  from (
SELECT	DISTINCT SMCo, Agreement, [Version],  
		GLDept = STUFF(( SELECT	distinct ',' + [udGLDept]
				FROM	@uGLAgDept2 AS B2
				WHERE B2.Agreement = B1.Agreement and B2.[Version]=B1.[Version] 	
				--ORDER BY Agreement, [Version] 
				FOR XML	PATH('')
				), 1, 1, '')
				,		
		DeptDesciption = STUFF(( SELECT distinct	',' + [DeptDescription]
				FROM	@uGLAgDept2 AS B2
				WHERE B2.Agreement = B1.Agreement and B2.[Version]=B1.[Version]  	
				--ORDER BY Agreement, [Version] 
				FOR XML	PATH('')
				), 1, 1, '')
				,
		Division = STUFF(( SELECT distinct	',' + [Division]
				FROM	@uGLAgDept2 AS B2
				WHERE B2.Agreement = B1.Agreement and B2.[Version]=B1.[Version] 
				--ORDER BY Agreement, [Version] 
				FOR XML	PATH('')
				), 1, 1, '')
				,
		ServiceSite = STUFF(( SELECT distinct	',' + ServiceSite
				FROM	@uGLAgDept2 AS B2
				WHERE B2.Agreement = B1.Agreement and B2.[Version]=B1.[Version]  	
				--ORDER BY Agreement, [Version] 
				FOR XML	PATH('')
				), 1, 1, '')
				,
		ServiceCenter = STUFF(( SELECT distinct	',' + ServiceCenter
				FROM	@uGLAgDept2 AS B2
				WHERE B2.Agreement = B1.Agreement and B2.[Version]=B1.[Version]  	
				--ORDER BY Agreement, [Version] 
				FOR XML	PATH('')
				), 1, 1, '')

 from @uGLAgDept2 as B1 )  q
 
 --select * from @uGLAgDept where Agreement in (select Agreement from @SMASdup) --debug test




