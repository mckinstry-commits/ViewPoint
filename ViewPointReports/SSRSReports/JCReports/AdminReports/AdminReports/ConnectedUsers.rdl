    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <CommandText>select 
    db_name(dbid) as [Database Name], 
    count(dbid) as [No Of Connections],
    loginame as [Login Name]
from
    sys.sysprocesses
where 
    dbid &gt; 0
and (db_name(dbid) = 'Viewpoint'
or db_name(dbid) = 'VPIntegration'
or db_name(dbid) = 'VPSample'
or db_name(dbid) = 'ViewpointPayroll')
group by 
    dbid, loginame</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Database_Name">
          <DataField>Database Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_Of_Connections">
          <DataField>No Of Connections</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Login_Name">
          <DataField>Login Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
