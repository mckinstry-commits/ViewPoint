  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhaseGroup">
            <Value>=Parameters!PhaseGroup.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Job">
            <Value>=Parameters!Job.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT jp.Job, jp.Phase, jp.Description AS PhaseDescription
	, ISNULL(pm.Phase, 'NO Phase Master') AS [MasterPhase], ISNULL(pm.Description,'INVALID MASTER PHASE') AS MasterDescription
	, fn.level, fn.Phase AS MasterParent,dv2.Description AS MasterParentDescription, fn.Parent
	, dv.Phase AS UpLvlPhase, dv.Description AS UpLvlDescription, cp.CostType
	, SUM(cp.ActualCost) AS ActualCost, SUM(cp.CurrEstCost) AS CurrentEstCost, SUM(cp.ForecastCost) AS ForecastCost, SUM(cp.OrigEstCost) AS OrigEstCost, SUM(cp.ProjCost) AS ProjectedCost, SUM(cp.TotalCmtdCost) AS TotalCmtdCost, SUM(cp.RemainCmtdCost) AS RemainingCost, SUM(cp.RecvdNotInvcdCost) AS RecvdNotInvcdCost
FROM JCJP jp
INNER JOIN JCCP cp ON cp.JCCo = jp.JCCo AND cp.Job = jp.Job AND cp.Phase = jp.Phase
LEFT OUTER JOIN JCJM jm ON jm.JCCo = jp.JCCo AND jm.Job = jp.Job
LEFT OUTER JOIN JCPM pm ON pm.PhaseGroup = jp.PhaseGroup AND LEFT(pm.Phase,10) = LEFT(jp.Phase,10)
LEFT OUTER JOIN (SELECT * FROM mckfnPhaseHeirarchy('')) fn ON fn.Phase = pm.udParentPhase
LEFT OUTER JOIN udPhaseDIV dv ON dv.Phase = fn.Parent
LEFT OUTER JOIN udPhaseDIV dv2 ON dv2.Phase = fn.Phase
WHERE jp.PhaseGroup = @PhaseGroup AND jp.Job = @Job
	AND ActiveYN = 'Y'
GROUP BY jp.Job, jp.Phase, jp.Description
	, ISNULL(pm.Phase, 'NO Phase Master') , ISNULL(pm.Description,'INVALID MASTER PHASE')
	, fn.level, fn.Phase,dv2.Description, fn.Parent
	, dv.Phase, dv.Description, cp.CostType</CommandText>
      </Query>
