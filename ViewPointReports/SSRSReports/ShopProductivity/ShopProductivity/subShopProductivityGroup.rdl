  <DataSets>
    <DataSet Name="ShopProductivityGroup">
      <Query>
        <DataSourceName>Viewpoint</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PeriodBegin">
            <Value>=Parameters!PeriodBegin.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PeriodEnd">
            <Value>=Parameters!PeriodEnd.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Groups">
            <Value>=Parameters!Groups.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE
	@TotalHours float,
	@TotalCost money

SELECT
	@TotalHours = [TotalHours],
	@TotalCost = [TotalCost]
FROM
	dbo.mfnShopProductivityTotals(@PeriodBegin, @PeriodEnd, 'base')

EXEC dbo.mspShopProductivityGroup @PeriodBegin, @PeriodEnd, @Groups, @TotalHours, @TotalCost</CommandText>
