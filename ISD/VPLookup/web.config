    <WhereClauses>
      <WhereClause Apply="false">
        <LHS>CompanyIDForMultiTenantApps1</LHS>
        <RHS>GetColumnValue("User",UserID(),"CompanyID")</RHS>
        <Operand>=</Operand>
      </WhereClause>
      <WhereClause Apply="false">
        <LHS>CompanyIDForMultiTenantApps2</LHS>
        <RHS>Session("CompanyID")</RHS>
        <Operand>=</Operand>
      </WhereClause>
    </WhereClauses>
    <Formulas>
